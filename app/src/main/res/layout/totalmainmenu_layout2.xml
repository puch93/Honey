        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/getmarried_bg_pop_background_190902"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/color_f6f6f6"
            android:orientation="vertical"
            android:padding="15dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <LinearLayout
                    android:id="@+id/ll_notice"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@drawable/sel_menu_bg"
                    android:gravity="center_horizontal"
                    android:orientation="vertical"
                    android:paddingTop="9dp"
                    android:paddingBottom="9dp">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="18dp"
                        android:adjustViewBounds="true"
                        android:src="@drawable/more_menu_01" />

                    <TextView
                        android:textStyle="bold"
                        android:includeFontPadding="false"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="8.3dp"
                        android:text="공지사항"
                        android:textColor="@color/color_4d4d4d"
                        android:textSize="11dp" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_block_know_member"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_weight="1"
                    android:background="@drawable/sel_menu_bg"
                    android:gravity="center_horizontal"
                    android:orientation="vertical"
                    android:paddingTop="9dp"
                    android:paddingBottom="9dp">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="18dp"
                        android:adjustViewBounds="true"
                        android:src="@drawable/more_menu_02" />

                    <TextView
                        android:textStyle="bold"
                        android:includeFontPadding="false"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="8.3dp"
                        android:text="지인차단"
                        android:textColor="@color/color_4d4d4d"
                        android:textSize="11dp" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_review"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_weight="1"
                    android:background="@drawable/sel_menu_bg"
                    android:gravity="center_horizontal"
                    android:orientation="vertical"
                    android:paddingTop="9dp"
                    android:paddingBottom="9dp">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="18dp"
                        android:adjustViewBounds="true"
                        android:src="@drawable/more_menu_03" />

                    <TextView
                        android:textStyle="bold"
                        android:includeFontPadding="false"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="8.3dp"
                        android:text="이용후기"
                        android:textColor="@color/color_4d4d4d"
                        android:textSize="11dp" />
                </LinearLayout>
            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:orientation="horizontal">

                <LinearLayout
                    android:id="@+id/ll_profile_revise"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@drawable/sel_menu_bg"
                    android:gravity="center_horizontal"
                    android:orientation="vertical"
                    android:paddingTop="9dp"
                    android:paddingBottom="9dp">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="18dp"
                        android:adjustViewBounds="true"
                        android:src="@drawable/more_menu_04" />

                    <TextView
                        android:textStyle="bold"
                        android:includeFontPadding="false"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="8.3dp"
                        android:text="정보수정"
                        android:textColor="@color/color_4d4d4d"
                        android:textSize="11dp" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_block_member"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_weight="1"
                    android:background="@drawable/sel_menu_bg"
                    android:gravity="center_horizontal"
                    android:orientation="vertical"
                    android:paddingTop="9dp"
                    android:paddingBottom="9dp">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="18dp"
                        android:adjustViewBounds="true"
                        android:src="@drawable/more_menu_05" />

                    <TextView
                        android:textStyle="bold"
                        android:includeFontPadding="false"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="8.3dp"
                        android:text="차단회원"
                        android:textColor="@color/color_4d4d4d"
                        android:textSize="11dp" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_buy_item"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_weight="1"
                    android:background="@drawable/sel_menu_bg"
                    android:gravity="center_horizontal"
                    android:orientation="vertical"
                    android:paddingTop="9dp"
                    android:paddingBottom="9dp">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="18dp"
                        android:adjustViewBounds="true"
                        android:src="@drawable/more_menu_06" />

                    <!--아이템-->
                 @color/color_4d4d4d-->
                    <TextView
                        android:textStyle="bold"
                        android:includeFontPadding="false"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="8.3dp"
                        android:text="충전/관리"
                        android:textColor="@color/color_4d"
                        android:textSize="11dp" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll_service_center"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@drawable/sel_menu_bg"
                    android:gravity="center_horizontal"
                    android:orientation="vertical"
                    android:paddingTop="9dp"
                    android:paddingBottom="9dp">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="18dp"
                        android:adjustViewBounds="true"
                        android:src="@drawable/more_menu_07" />

                    <TextView
                        android:textStyle="bold"
                        android:includeFontPadding="false"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="8.3dp"
