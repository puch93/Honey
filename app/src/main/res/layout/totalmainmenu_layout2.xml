                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="8.3dp"
                        android:text="알람설정"
                        android:textColor="@color/color_4d4d4d"
                        android:textSize="11dp" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_invite_message"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_weight="1"
                    android:background="@drawable/sel_menu_bg"
                    android:gravity="center_horizontal"
                    android:orientation="vertical"
                    android:paddingTop="9dp"
                    android:paddingBottom="9dp">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="18dp"
                        android:adjustViewBounds="true"
                        android:src="@drawable/more_menu_12" />

                    <TextView
                        android:textStyle="bold"
                        android:includeFontPadding="false"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="8.3dp"
                        android:text="문자초대"
                        android:textColor="@color/color_4d4d4d"
                        android:textSize="11dp" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:orientation="horizontal">

                <LinearLayout
                    android:id="@+id/ll_invite_kakao"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@drawable/sel_menu_bg"
                    android:gravity="center_horizontal"
                    android:orientation="vertical"
                    android:paddingTop="9dp"
                    android:paddingBottom="9dp">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="18dp"
                        android:adjustViewBounds="true"
                        android:src="@drawable/more_menu_13" />

                    <TextView
                        android:textStyle="bold"
